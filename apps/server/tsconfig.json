{
  "extends": "tsconfig/nestjs-server.json",
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist",
    "paths": {
      "@common/*": ["src/common/*"],
      "@providers/*": ["src/providers/*"],
      "@models/*": ["src/models/*"],
      "@config/*": ["src/config/*"]
    }
  },
  "ts-node": {
    "require": ["tsconfig-paths/register"]
  }
}
