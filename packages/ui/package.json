{
  "name": "ui",
  "private": true,
  "version": "0.0.0",
  "sideEffects": [
    "**/*.css"
  ],
  "type": "module",
  "main": "./dist/index.es.js",
  "module": "./index.ts",
  "types": "./dist/index.d.js",
  "exports": {
    ".": "./dist/index.es.js",
    "./style": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite build --watch",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives",
