{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"]
    },
    "pokemon-info#build": {
      "dependsOn": ["contract#build"]
    },
    "lint": {
      "cache": false
    },
    "check-types": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    },
    "studio": {
      "cache": false,
      "persistent": true
    },
    "studio:prod": {
      "cache": false,
      "persistent": true
    },
    "prisma:generate": {
      "cache": false
    },
    "prisma:migrate": {
      "cache": false
    },
    "db:seed": {
      "cache": false
    },
    "prisma:migrate:prod": {
      "cache": false
    },
    "db:seed:prod": {
      "cache": false
    },
    "prettier:format": {
      "cache": false
    },
    "test": {
      "cache": false,
      "persistent": true
    }
  }
}
